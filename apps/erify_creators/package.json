{
  "name": "erify_creators",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "dev": "paraglide-js compile --project ./project.inlang --outdir ./src/paraglide && concurrently -n paraglide,vite -c cyan,magenta \"chokidar \\\"src/i18n/messages/**/*.json\\\" \\\"project.inlang/**/*\\\" -c \\\"paraglide-js compile --project ./project.inlang --outdir ./src/paraglide\\\"\" \"vite\"",
    "build": "paraglide-js compile --project ./project.inlang --outdir ./src/paraglide && vite build",
    "lint": "eslint . --fix",
    "preview": "vite preview",
    "start": "npx serve -s ./dist",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@eridu/api-types": "workspace:*",
    "@eridu/auth-sdk": "workspace:*",
    "@eridu/i18n": "workspace:*",
    "@eridu/ui": "workspace:*",
    "@tanstack/react-query": "^5.90.11",
    "@tanstack/react-query-devtools": "^5.91.1",
    "@tanstack/react-query-persist-client": "^5.90.13",
    "@tanstack/react-router": "^1.139.3",
    "@tanstack/react-router-devtools": "^1.139.3",
    "@tanstack/react-table": "^8.21.3",
    "axios": "^1.13.2",
    "date-fns": "^4.1.0",
    "idb-keyval": "^6.2.2",
    "jose": "^6.0.11",
    "lucide-react": "^0.554.0",
    "react": "^19.2.0",
    "react-day-picker": "^9.11.3",
    "react-dom": "^19.2.0",
    "serve": "^14.2.5",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@eridu/eslint-config": "workspace:*",
    "@eridu/typescript-config": "workspace:*",
    "@eslint/js": "^9.39.1",
    "@inlang/paraglide-js": "^2.0.0",
    "@tailwindcss/vite": "^4.1.17",
    "@tanstack/router-plugin": "^1.139.3",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/coverage-v8": "^4.0.13",
    "@vitest/ui": "^4.0.13",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "happy-dom": "^20.0.11",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "vitest": "^4.0.13"
  }
}

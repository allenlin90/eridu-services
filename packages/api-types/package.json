{
  "name": "@eridu/api-types",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "description": "Shared API types and schemas for eridu-services monorepo",
  "engines": {
    "node": ">=22"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./clients": {
      "types": "./dist/clients/index.d.ts",
      "default": "./dist/clients/index.js"
    },
    "./constants": {
      "types": "./dist/constants.d.ts",
      "default": "./dist/constants.js"
    },
    "./pagination": {
      "types": "./dist/pagination/index.d.ts",
      "default": "./dist/pagination/index.js"
    },
    "./show-standards": {
      "types": "./dist/show-standards/index.d.ts",
      "default": "./dist/show-standards/index.js"
    },
    "./show-statuses": {
      "types": "./dist/show-statuses/index.d.ts",
      "default": "./dist/show-statuses/index.js"
    },
    "./show-types": {
      "types": "./dist/show-types/index.d.ts",
      "default": "./dist/show-types/index.js"
    },
    "./shows": {
      "types": "./dist/shows/index.d.ts",
      "default": "./dist/shows/index.js"
    },
    "./studio-rooms": {
      "types": "./dist/studio-rooms/index.d.ts",
      "default": "./dist/studio-rooms/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch --preserveWatchOutput",
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "test": "echo \"No tests specified\" && exit 0"
  },
  "dependencies": {
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@eridu/eslint-config": "workspace:*",
    "@eridu/typescript-config": "workspace:*",
    "@types/node": "^24.10.1",
    "eslint": "^9.39.1",
    "typescript": "^5.9.3"
  }
}
